{
  "name": "nodejstemplate",
  "version": "1.1.0",
  "description": "Simple Nodejs template",
  "main": "src/index.js",
  "type": "module",
  "scripts": {
    "lint": "eslint src/**/*.js --fix",
    "dev": "nodemon src/index.js",
    "prettier": "prettier --write .",
    "prepare": "husky install",
    "postinstall": "npm run prepare",
    "correct": "npm run lint & npm run prettier"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/LopCuns/NODEJSTEMPLATE.git"
  },
  "author": "LopCuns",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/LopCuns/NODEJSTEMPLATE/issues"
  },
  "homepage": "https://github.com/LopCuns/NODEJSTEMPLATE#readme",
  "devDependencies": {
    "husky": "8.0.3",
    "lint-staged": "13.2.2",
    "nodemon": "2.0.22",
    "prettier": "2.8.8",
    "standard": "17.1.0"
  },
  "imports": {
    "#Config/*": "./src/config/*",
    "#Constants/*": "./src/constants/*",
    "#Controllers/*": "./src/controllers/*",
    "#Dto/*": "./src/dto/*",
    "#Errors/*": [
      "./src/errors/*"
    ],
    "#Routes/*": "./src/routes/*",
    "#Schemas/*": "./src/schemas/*"
  },
  "eslintConfig": {
    "extends": "./node_modules/standard/eslintrc.json"
  },
  "lint-staged": {
    "*.js": "eslint --fix",
    "*.{js,md}": "prettier --write"
  },
  "dependencies": {
    "ajv": "^8.12.0",
    "ajv-errors": "^3.0.0",
    "ajv-formats": "^2.1.1",
    "bcrypt": "^5.1.0",
    "cors": "^2.8.5",
    "dotenv": "^16.1.4",
    "express": "^4.18.2",
    "jose": "^4.14.4",
    "mongoose": "^7.2.3"
  }
}
